	    <span class="n">temp</span><span class="o">=</span><span class="n">root</span><span class="o">-&gt;</span><span class="n">RNode</span><span class="o">-&gt;</span><span class="n">LeftMaxLen</span><span class="p">;</span>
	    <span class="k">else</span>
