	<span class="p">}</span>

	<span class="k">if</span><span class="p">(</span><span class="n">root</span><span class="o">-&gt;</span><span class="n">LeftMaxLen</span> <span class="o">+</span> <span class="n">root</span><span class="o">-&gt;</span><span class="n">RightMaxLen</span> <span class="o">&gt;</span> <span class="n">MaxLen</span><span class="p">)</span>
